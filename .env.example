# 데이터베이스 설정
DB_HOST=localhost
DB_PORT=3306
DB_NAME=drone_delivery
DB_USERNAME=root
DB_PASSWORD=your_password_here

# Spring 프로파일 (local, docker, prod)
SPRING_PROFILES_ACTIVE=local

# 서버 포트
SERVER_PORT=8080

# JPA 설정
SPRING_JPA_DDL_AUTO=none
SPRING_JPA_SHOW_SQL=true

# 로그 레벨 (debug, info, warn, error)
LOG_LEVEL=debug
